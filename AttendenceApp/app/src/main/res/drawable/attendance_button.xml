<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="2000dp" />
            <stroke android:width="1dp" android:color="#6dd409ae" />
            <gradient android:angle="-90" android:startColor="#6db90898" android:endColor="#6dc1069e"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="2000dp" />
            <stroke android:width="1dp" android:color="#86ef07c4" />
            <solid android:color="#88af0990"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="2000dp" />
            <stroke android:width="1dp" android:color="#89c406a1" />
            <gradient android:angle="-90" android:startColor="#aeef07c4" android:endColor="#a5c209a0" />
        </shape>
    </item>
</selector>